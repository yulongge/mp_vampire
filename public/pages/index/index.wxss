/*由 css\pages\index\index.less 生成，请勿手动修改以免被覆盖！*/
page {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  bottom: 0rpx;
  right: 0rpx;
  background: linear-gradient(to bottom, #00688B, #008B8B);
}
page .main {
  width: 100%;
  overflow: auto;
  height: 100vh;
  padding: 0rpx 20rpx 20rpx 20rpx;
  box-sizing: border-box;
}
page .main::-webkit-scrollbar {
  display: none;
}
page .focus {
  position: fixed;
  bottom: 20rpx;
  width: 100%;
  padding: 0rpx 20rpx;
  box-sizing: border-box;
}
page .focus .con {
  box-sizing: border-box;
}
page .search {
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}
page .search input {
  background-color: #fff;
  border-radius: 40rpx;
  padding: 0rpx 20rpx;
  font-size: 26rpx;
  height: 60rpx;
  margin-top: 20rpx;
}
